{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.cache": true,
    "**/.vs/": true,
    "**/*.cs": true,
    "**/*.orig": true,
    "**/bin/": true,
    "**/build/": true,
    "**/build/**/*": true,
    "**/debug/": true,
    "**/dist/": true,
    "**/obj": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    ".yarn/releases": true,
    ".yarn/cache": true,
    ".yarn/plugins": true
  },
  "explorerExclude.backup": {},
  "eslint.enable": true,
  "eslint.workingDirectories": [
    { "directory": "packages/frontend", "changeProcessCWD": true },
    { "directory": "packages/hardhat", "changeProcessCWD": true }
  ]
}
